<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProjectGuid>{ACAEF8EC-A3E5-4BB4-993F-BEE7FBD0A92A}</ProjectGuid>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SolutionDir Condition="$(SolutionDir) == '' Or $(SolutionDir) == '*Undefined*'">..\</SolutionDir>
    <AssemblyTitle>Toscana</AssemblyTitle>
    <Product>Toscana</Product>
    <Description>Toscana, which stands for TOSCA Net Analyzer, is a .NET library for validating, parsing and analyzing TOSCA YAML format.</Description>
    <Copyright>Copyright © 2016</Copyright>
    <Version>1.0.0.0</Version>
    <DocumentationFile>bin\$(Configuration)\Toscana.xml</DocumentationFile>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <Authors>Boris Modylevsky</Authors>
    <PackageId>Toscana</PackageId>
    <PackageLicenseUrl>https://github.com/QualiSystems/Toscana/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/qualisystems/toscana</PackageProjectUrl>
    <PackageTags>cloud orchestration TOSCA OASIS YAML yml</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject />
  </PropertyGroup>
  <PropertyGroup>
    <SignAssembly>True</SignAssembly>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>..\Toscana.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DataAnnotationsValidatorNetStandard" Version="[1.0.3,2.0.0)" />
    <PackageReference Include="QuickGraph.NETStandard" Version="[3.8.0,4.0.0)" />
    <PackageReference Include="System.IO.Abstractions" Version="[19.2.29,20.0.0.0)" />
    <PackageReference Include="YamlDotNet" Version="[13.1.1,999.0.0)" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\Toscana.snk">
      <Link>Toscana.snk</Link>
    </None>
  </ItemGroup>
</Project>